{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "dev": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": true
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": true
    }
  },
  "defaultBase": "master"
}
